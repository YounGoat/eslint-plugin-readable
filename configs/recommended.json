{
	"rules": {
		"readable/const-name": "error",
		"readable/function-name": "error",
		"readable/function-lines": ["error", {
			"max": 40,
			"skipComments": true,
			"skipBlankLines": true
		}],
		"readable/loop": ["error", {
			"maxDepth": 3
		}],
		"readable/parameters": ["error", {
			"maxLength": 4
		}],
		"readable/property-name": "error",
		"readable/var-name": "error"
	}
}
